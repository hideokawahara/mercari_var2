.header__inside
  .header__inside__main
    %h1.icon
      %a{ href: '/'}
        %img{ src: "assets/material/logo/logo-white.png", alt: "ピカわか", style: "filter: brightness(0);" }
    .header__inside__main__search
      = form_with url: '#' do |form|
        = form.text_field :searchInput, placeholder: "キーワードから探す", class: "searchBox--input"
        -# = form.submit
        %button{ type: "submit"} 
          .searchBox__icon
            %img{ src: "assets/material/icon/icon-search 1.png"}
    %ul.resultLists
  .header__inside__nav
    %ul.leftPart
      %li.leftPart__item.leftPart__item--first
        %a#catBtn{href: "#"} カテゴリー
        .tree.displayNone
          /
            <ul class="categoryTree">
            <li class="categoryTree--item"><a id="1" href="#">レディース</a></li>
            <li class="categoryTree--item"><a id="2" href="#">メンズ</a></li>
            <li class="categoryTree--item"><a id="3" href="#">ベビー・キッズ</a></li>
            <li class="categoryTree--item"><a id="4" href="#">インテリア・住まい・小物</a></li>
            <li class="categoryTree--item"><a id="5" href="#">本・音楽・ゲーム</a></li>
            <li class="categoryTree--item"><a id="6" href="#">おもちゃ・ホビー・グッズ</a></li>
            <li class="categoryTree--item"><a id="7" href="#">コスメ・香水・美容</a></li>
            <li class="categoryTree--item"><a id="8" href="#">家電・スマホ・カメラ</a></li>
            <li class="categoryTree--item"><a id="9" href="#">スポーツ・レジャー</a></li>
            <li class="categoryTree--item"><a id="10" href="#">ハンドメイド</a></li>
            <li class="categoryTree--item"><a id="11" href="#">チケット</a></li>
            <li class="categoryTree--item"><a id="12" href="#">自動車・オートバイ</a></li>
            <li class="categoryTree--item"><a id="13" href="#">その他</a></li>
          / </ul>
      %li.leftPart__item
        %a#brandBtn{href: "#"} ブランド
        %ul.brandsPulldown.displayNone
    %ul.rightPart
      %li.rightPart__item.rightPart__item--login
        %a{href: "#"} ログイン
      %li.rightPart__item.rightPart__item--new
        %a{href: "#"} 新規会員登録
